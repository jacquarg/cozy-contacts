[
  {
    "name": "docType",
    "type": "Template",
    "value": "Contact"
  },
  {
     "name": "__first_name",
     "type": "First Name"
  },
  {
     "name": "__last_name",
     "type": "Last Name"
  },
  {
     "name": "fn",
     "type": "Full Name"
  },
  {
     "name": "n",
     "type": "Template",
     "value": "{__last_name};{__first_name};;;"
  },
  {
     "name": "notes",
     "type": "Sentences",
     "min": 1,
     "max": 3,
     "percentBlank": 70
  },
  {
     "name": "__phone[8]",
     "type": "Phone"
  },
  {
     "name": "__email[8]",
     "type": "Email Address"
  },
  {
     "name": "__addr_street",
     "type": "Street Address"
  },
  {
     "name": "__addr_city",
     "type": "City"
  },
  {
     "name": "__addr_pc",
     "type": "Postal Code"
  },
  {
     "name": "__addr_country",
     "type": "Country"
  },
  {
     "name": "datapoints",
     "type": "JSON Array",
     "min": 1,
     "max": 8
  },
  {
     "name": "datapoints.name",
     "type": "Custom List",
     "values": ["tel", "email", "adr"]
  },
  {
     "name": "datapoints.type",
     "type": "Custom List",
     "values": ["home", "work"]
  },
  {
     "name": "datapoints.value",
     "type": "Formula",
     "value": "case name when 'tel' then __phone[random(0,7)] when 'email' then __email[random(0,7)] else concat(__addr_street, ', ', __addr_pc, ' ', __addr_city, ', ', __addr_country) end"
  }
]
